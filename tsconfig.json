{
  "include": ["src/ts/*.ts"],
  "compilerOptions": {
    "target": "ES6",
    "allowJs": true,
    "checkJs": true,
    "outDir": "build/js",
    "noImplicitAny": false,
    "types": ["network-information-types", "user-agent-data-types", "jquery"],
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "allowUnreachableCode": false,
    "noStrictGenericChecks": true
  }
}
